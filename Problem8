#include <bits/stdc++.h>

#define fi first
#define se second
#define ll long long
#define sz(x) (int)x.size()
#define all(x) x.begin(), x.end()
 
using namespace std;
 
const int E9 = (int) 1e9;
const int N = (int) 5e5+7;
const ll INF = (ll) 1e18;

// complexity: O(N)

bool solve(string &s)
{
    for (int i = 0; i < sz(s); ++i)
    {
        if ('0' > s[i] || s[i] > '9') return false;
    }
    return true;
}

int main() 
{
    string s;
    cin >> s;
    cout << (solve(s) ? "Yes" : "No");
}
