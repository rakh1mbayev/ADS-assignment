#include <bits/stdc++.h>

#define fi first
#define se second
#define ll long long
#define sz(x) (int)x.size()
#define all(x) x.begin(), x.end()
 
using namespace std;
 
const int E9 = (int) 1e9;
const int N = (int) 5e5+7;
const ll INF = (ll) 1e18;

// complexity: O(!N)

void solve(string &s)
{
    sort(s.begin(), s.end());
    do
    {
        std::cout << s << '\n';
    }while(std::next_permutation(s.begin(), s.end()));
}

int main() 
{
    string s;
    cin >> s;
    solve(s);
}
